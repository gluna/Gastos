<p id="notice"><%= notice %></p>

<h1>Receita</h1>

<table>
  <thead>
    <tr>
      <th>Dtreceita</th>
      <th>Tipo receita</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<%= link_to 'New Receita', new_receitum_path %>

  <tbody>
    <% @receita.each do |receita| %>
      <tr>
        <td><%= receita.dtreceita %></td>
        <td><%= receita.tipo_receita.descricao %></td>
        <td><%= receita.valor %></td>
        <td><%= link_to 'Show', receita %></td>
        <td><%= link_to 'Edit', edit_receitum_path(receita) %></td>
        <td><%= link_to 'Destroy', receita, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
