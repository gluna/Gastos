<p id="notice"><%= notice %></p>

<h1>Tipo Gastos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_gastos.each do |tipo_gasto| %>
      <tr>
        <td><%= tipo_gasto.descricao %></td>
        <td><%= link_to 'Show', tipo_gasto %></td>
        <td><%= link_to 'Edit', edit_tipo_gasto_path(tipo_gasto) %></td>
        <td><%= link_to 'Destroy', tipo_gasto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Gasto', new_tipo_gasto_path %>
