<p id="notice"><%= notice %></p>

<h1>Tipo Receita</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<%= link_to 'New Tipo Receita', new_tipo_receitum_path %>

  <tbody>
    <% @tipo_receita.each do |tipo_receita| %>
      <tr>
        <td><%= tipo_receita.descricao %></td>
        <td><%= link_to 'Show', tipo_receita %></td>
        <td><%= link_to 'Edit', edit_tipo_receitum_path(tipo_receita) %></td>
        <td><%= link_to 'Destroy', tipo_receita, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
